{
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_title": "cs61a.org Dark Mode (Enabled)"
    },
    "content_scripts": [{
	"all_frames": false,
        "js": ["js/content.js"],
        "matches": ["*://cs61a.org/*"],
	"run_at": "document_start"
    }],
    "description": "Extension for toggling dark mode on cs61a.org",
    "icons": {
        "48": "icons/icon48.png",
        "96": "icons/icon96.png"
    },
    "manifest_version": 2,
    "name": "CS61A Dark Mode",
    "permissions": ["tabs"],
    "version": "1.0.0",
    "web_accessible_resources": ["css/override.css"]
}